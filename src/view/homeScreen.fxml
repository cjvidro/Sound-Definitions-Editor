<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="650.0" prefWidth="943.0">
      <children>
            <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="650.0" prefWidth="943.0">
               <children>
                  <VBox alignment="TOP_CENTER" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="500.0" spacing="20.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets top="40.0" />
                     </padding>
                     <children>
                        <Label alignment="CENTER" text="JSON Sound Definitions Editor" textAlignment="CENTER">
                           <font>
                              <Font size="25.0" />
                           </font>
                        </Label>
                        <VBox alignment="TOP_CENTER" fillWidth="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="160.0" prefWidth="450.0" spacing="10.0" style="-fx-background-color: lightgray;">
                           <children>
                              <Label text="Get Started">
                                 <font>
                                    <Font size="20.0" />
                                 </font>
                              </Label>
                              <Label text="Click below to open or create a new project!" wrapText="true">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </Label>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </VBox>
                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <Label text="Login With Google">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                     </children>
                  </VBox>
                  <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="155.0" prefWidth="943.0" spacing="15.0">
                     <children>
                        <Button fx:id="openProjectButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#importSoundDefinitions" prefHeight="125.0" prefWidth="125.0">
                           <graphic>
                              <ImageView fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@folder.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                        <HBox prefHeight="125.0" spacing="2.0">
                           <children>
                              <Button fx:id="project1Button" disable="true" mnemonicParsing="false" prefHeight="125.0" prefWidth="125.0" textAlignment="CENTER" wrapText="true" />
                              <Button fx:id="project2Button" disable="true" mnemonicParsing="false" prefHeight="125.0" prefWidth="125.0" textAlignment="CENTER" wrapText="true" />
                              <Button fx:id="project3Button" disable="true" mnemonicParsing="false" prefHeight="125.0" prefWidth="125.0" textAlignment="CENTER" wrapText="true" />
                              <Button fx:id="project4Button" disable="true" mnemonicParsing="false" prefHeight="125.0" prefWidth="125.0" textAlignment="CENTER" wrapText="true" />
                              <Button fx:id="project5Button" disable="true" mnemonicParsing="false" prefHeight="125.0" prefWidth="125.0" textAlignment="CENTER" wrapText="true" />
                           </children>
                        </HBox>
                        <Button fx:id="NewProjectButton" mnemonicParsing="false" onAction="#newProject" prefHeight="125.0" prefWidth="125.0">
                           <graphic>
                              <ImageView fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@plus.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                     <padding>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
      </children>
      </AnchorPane>
   </center>
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Region maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="1.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </top>
   <bottom>
      <VBox>
         <children>
            <Region maxHeight="1.7976931348623157E308" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="1.0" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </bottom>
</BorderPane>
