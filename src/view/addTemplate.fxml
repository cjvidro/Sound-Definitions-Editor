<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <AnchorPane minHeight="-1.0" minWidth="-1.0">
         <children>
            <VBox spacing="10.0">
               <children>
                  <HBox alignment="CENTER">
                     <children>
                        <Label alignment="CENTER" text="Add New Template" textAlignment="CENTER" underline="true">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Template Name">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Region HBox.hgrow="ALWAYS" />
                        <TextField />
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Category">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Region HBox.hgrow="ALWAYS" />
                        <ComboBox prefWidth="150.0" promptText="Select Category" />
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Min Distance">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Region HBox.hgrow="ALWAYS" />
                        <TextField promptText="1.0" />
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Max Distance">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Region HBox.hgrow="ALWAYS" />
                        <TextField promptText="1.0" />
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Stream">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Region prefWidth="125.0" />
                        <HBox>
                           <children>
                              <CheckBox mnemonicParsing="false">
                                 <font>
                                    <Font size="16.0" />
                                 </font>
                              </CheckBox>
                           </children>
                        </HBox>
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Volume">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Region HBox.hgrow="ALWAYS" />
                        <TextField promptText="1.0" />
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Pitch">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Region HBox.hgrow="ALWAYS" />
                        <TextField promptText="1.0" />
                     </children>
                  </HBox>
                  <HBox spacing="10.0">
                     <children>
                        <Label text="Low Memory Load">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Label>
                        <Region prefWidth="45.0" />
                        <CheckBox mnemonicParsing="false">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </CheckBox>
                        <Region HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <HBox alignment="TOP_CENTER" spacing="15.0">
                     <children>
                        <Button fx:id="addTemplateSave" mnemonicParsing="false" onAction="#saveAddTemplate" text="Save">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                        <Button fx:id="addTemplateCancel" mnemonicParsing="false" onAction="#cancelAddTemplate" text="Cancel">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </padding>
            </VBox>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
